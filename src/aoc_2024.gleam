pub fn main() {
  // Sort each list
  let lists = split_pairs_into_separate_lists(pairs)
  let sorted = #(
    list.sort(lists.0, int.compare),
    list.sort(lists.1, int.compare),
  )

  let increment = fn(x) {
    case x {
      option.Some(value) -> value + 1
      option.None -> 1
    }
  }
  let freq_d =
    list.fold(lists.1, dict.new(), fn(dic, in) {
      dict.upsert(dic, in, increment)
    })

  freq_d
  |> dict.to_list
  |> list.filter(fn(v) { v.1 != 1 })
  |> io.debug

  let freq_value = fn(i: Int) -> Int {
    let freq_v = dict.get(freq_d, i)
    case freq_v {
      Ok(v) -> i * v
      Error(_) -> 0
    }
  }

  lists.0
  |> list.map(freq_value)
  |> list.fold(0, fn(x, y) { x + y })
  |> int.to_string
  |> io.println
  // Iterate over the range of lists, calculating the diff
  //get_diff(sorted.0, sorted.1, 0)
  //|> int.to_string
  //|> io.println
}

fn get_diff(l1, l2, accum) {
  case l1, l2 {
    [item1, ..rest1], [item2, ..rest2] ->
      get_diff(rest1, rest2, accum + int.absolute_value({ item1 - item2 }))
    [], [] -> accum
    _, _ -> accum
  }
}

fn split_pairs(
  pairs: List(#(Int, Int)),
  running_lists: #(List(Int), List(Int)),
) -> #(List(Int), List(Int)) {
  case pairs {
    [first_pair, ..rest] ->
      split_pairs(
        rest,
        #([first_pair.0, ..running_lists.0], [first_pair.1, ..running_lists.1]),
      )
    _ -> running_lists
  }
}

fn split_pairs_into_separate_lists(pairs) {
  split_pairs(pairs, #([], []))
}
